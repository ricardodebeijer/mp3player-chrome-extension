{
  "manifest_version": 2,
  "name": "mp3player extension",
  "description": "This extension will be used in conjuction with mp3player",
  "version": "1.0",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://player.ricardodebeijer.nl/*",
    "http://localhost:8000/*",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}